{
  "name": "blockusign",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "copySignUpPage": "cp ./src/signup.html ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/",
    "buildprod": "npm run prod && cp -av ./www/* ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/ && npm run copySignUpPage",
    "buildweb": "ionic build && cp -av ./www/* ../BlockUSign.Backend/BlockUSign.Backend/wwwroot/ && npm run copySignUpPage",
    "prod": "ionic build --prod --aot --minifyjs --minifycss --optimizejs",
    "fast": "node ./scripts/server.js",
    "chrome": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/chrome.ps1",
    "chrome-private": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./scripts/chrome-private.ps1",
    "chrome-mac": "./scripts/chrome.sh",
    "chrome-mac-private": "./scripts/chrome-private.sh",
    "serve": "ionic build && node ./scripts/server.js",
    "private": "npm run chrome-mac-private && ionic serve",
    "start": "npm run chrome-mac && ionic serve"
  },
  "dependencies": {
    "@angular/common": "5.0.3",
    "@angular/compiler": "5.0.3",
    "@angular/compiler-cli": "5.0.3",
    "@angular/core": "5.0.3",
    "@angular/forms": "5.0.3",
    "@angular/http": "5.0.3",
    "@angular/platform-browser": "5.0.3",
    "@angular/platform-browser-dynamic": "5.0.3",
    "@ionic-native/core": "4.4.0",
    "@ionic-native/splash-screen": "4.4.0",
    "@ionic-native/status-bar": "4.4.0",
    "@ionic/storage": "2.1.3",
    "@ng-select/ng-select": "^1.5.0",
    "automerge": "^0.7.8",
    "babel-polyfill": "^6.26.0",
    "blockstack.ts": "^0.1.0",
    "bower": "^1.8.2",
    "chart.js": "^2.7.1",
    "cordova-android": "6.3.0",
    "cordova-plugin-device": "^1.1.4",
    "cordova-plugin-ionic-webview": "^1.1.16",
    "cordova-plugin-splashscreen": "^4.0.3",
    "cordova-plugin-whitelist": "^1.3.1",
    "eosjs": "^15.0.3",
    "eosjs-keygen": "^1.3.2",
    "express": "^4.16.3",
    "ionic-angular": "3.9.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
    "jslinq": "^1.0.21",
    "linq-to-typescript": "^2.0.0",
    "linqts": "^1.11.0",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.14",
    "ng2-charts": "^1.6.0",
    "openpgp": "^3.0.10",
    "opn": "^5.3.0",
    "pdf-annotate": "0.0.0",
    "pdfjs-dist": "^2.0.480",
    "requirejs": "^2.3.5",
    "rxjs": "^5.5.2",
    "rxjs-compat": "^6.1.0",
    "sw-toolbox": "3.6.0",
    "technicalindicators": "^1.1.1",
    "zone.js": "0.8.18"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.1.6",
    "typescript": "2.4.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "ionic-plugin-keyboard": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {}
    },
    "platforms": [
      "android"
    ]
  }
